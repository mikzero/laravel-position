{
    "name": "nevadskiy/laravel-position",
    "description": "The package allows you to arrange the laravel models in a given order.",
    "license": "MIT",
    "keywords": ["position", "sort", "order", "arrange", "eloquent", "laravel"],
    "authors": [
        {
            "name": "Nevadskiy",
            "email": "nevadskiy@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "laravel/framework": "^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "^5.0|^6.0",
        "nevadskiy/php-cs-fixer-rules": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Nevadskiy\\Position\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nevadskiy\\Position\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit --colors=always",
        "fix": "php-cs-fixer fix"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/nevadskiy/php-cs-fixer-rules.git"
        }
    ]
}
